{
  "name": "yassirawad/nova-theme",
  "description": "Laravel nova tool that provides multiple theme for nova cms",
  "keywords": [
    "laravel",
    "nova",
    "theme",
    "themes",
    "novathemes"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "inspheric/nova-url-field": "^1.32",
    "laravel/nova": "2.0.*",
    "metrixinfo/nova-iframe": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Yassir3wad\\NovaTheme\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Yassir3wad\\NovaTheme\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
